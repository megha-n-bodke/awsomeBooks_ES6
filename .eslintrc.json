{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "extends": ["airbnb-base"],
  "rules": {
    "import/no-cycle": "off",
    "import/extensions": "off",
    "lines-between-class-members": "off",
    "class-methods-use-this": "off",
    "no-undef": "off",
    "no-shadow": "off",
    "no-param-reassign": "off",
    "eol-last": "off"
  },
  "ignorePatterns": ["dist/", "build/"]
}
